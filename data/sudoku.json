{
  "$schema": "http://json-schema.org/schema#",
  "description": "Constraints in a sudoku game",
  "title": "Sudoku grid",
  "type": "array",
  "items": {
    "type": "object",
    "patternProperties": {
      "^[0-9]+ [0-9]+$": {
        "type": "integer"
      }
    },
    "additionalProperties": false
  }
}
